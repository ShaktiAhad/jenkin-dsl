pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                script{
                    def datas = readYaml file: "${WORKSPACE}/projects.yaml"
                    datas.each{tech, projects ->
                        println(tech)
                        for (each_project in projects){
                            println(each_project)
                        }
                    }
                }
            }
        }
    }
}