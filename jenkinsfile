pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                script{
                    def datas = readYaml file: "${WORKSPACE}/projects.yaml"
                    def createFolders = load "${WORKSPACE}/s.groovy"
                    createFolders("${WORKSPACE}/folder.groovy",datas)
                }
            }
        }
    }
}