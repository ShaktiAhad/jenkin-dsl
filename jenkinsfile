pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                script{
                    def datas = readYaml file: "${WORKSPACE}/projects.yaml"
                    def f = load "${WORKSPACE}/s.groovy"
                    f.folder("folder.groovy", datas)
                    // def f = "folder.groovy"
                    // jobDsl targets: f, additionalParameters: [folders: datas]
                }
            }
        }
    }
}